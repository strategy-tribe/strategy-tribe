{
  "name": "strategytribe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prettier": "prettier --write .",
    "prettier:check": "prettier -c .",
    "lint": "next lint",
    "lint:fix": "yarn prettier && yarn format && eslint src --fix",
    "lint:strict": "eslint --max-warnings=0 src",
    "lint:per": "lint:fix && eslint --format summary-chart src",
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "yarn prisma format",
    "generate": "yarn prisma format && yarn prisma generate",
    "generate:force": "yarn prisma format && yarn prisma db push && yarn prisma generate",
    "studio": "yarn prisma format && yarn prisma generate && yarn prisma studio",
    "studio:force": "yarn generate:force && yarn prisma studio",
    "test": "jest",
    "t": "jest"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.10.3",
    "@formkit/auto-animate": "^1.0.0-beta.3",
    "@headlessui/react": "^1.7.4",
    "@nivo/core": "^0.79.0",
    "@nivo/geo": "^0.79.1",
    "@prisma/client": "^4.6.1",
    "@tanstack/react-query": "^4.13.0",
    "@tanstack/react-table": "^8.2.6",
    "@trpc/client": "^10.0.0-rc.0",
    "@trpc/next": "^10.0.0-rc.0",
    "@trpc/react-query": "^10.0.0-rc.0",
    "@trpc/server": "^10.0.0-rc.0",
    "@types/react-datepicker": "^4.4.1",
    "aws-sdk": "^2.1248.0",
    "axios": "^1.1.3",
    "ethers": "^5.7.2",
    "framer-motion": "^6.3.3",
    "moralis": "^2.6.7",
    "next": "^13.0.0",
    "next-auth": "^4.15.0",
    "prisma": "^4.6.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.7.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "react-onesignal": "^2.0.3",
    "react-switch": "^7.0.0",
    "react-textarea-autosize": "^8.3.3",
    "superjson": "^1.11.0",
    "uuid": "^8.3.2",
    "wagmi": "^0.7.5",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@faker-js/faker": "^7.6.0",
    "@headlessui/tailwindcss": "^0.1.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.2",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-summary-chart": "^0.2.1",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-mock-extended": "2.0.4",
    "postcss": "^8.4.12",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.6",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.3"
  },
  "overrides": {
    "react-switch": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-onclickoutside": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "react-datepicker": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "use-immer": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  },
  "transform": {
    "\\.js$": "<rootDir>/node_modules/babel-jest"
  }
}
